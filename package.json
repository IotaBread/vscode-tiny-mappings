{
    "name": "vscode-tiny-mappings",
    "displayName": "Tiny Mappings for VSCode",
    "description": "Syntax highlighting for mapping files",
    "version": "0.2.3",
    "publisher": "ByMartrixX",
    "author": {
        "name": "ByMartrixx"
    },
    "license": "MIT",
    "keywords": [
        "tiny",
        "enigma",
        "mapping",
        "java",
        "fabric",
        "minecraft",
        "mc",
        "fabricmc"
    ],
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ByMartrixx/vscode-tiny-mappings.git"
    },
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tinyv2",
                "aliases": [
                    "Tiny V2 Mappings",
                    "tinyv2"
                ],
                "extensions": [
                    ".tiny"
                ],
                "configuration": "./tinyv2-language-configuration.json"
            },
            {
                "id": "enigma-mapping",
                "aliases": [
                    "Enigma Mappings",
                    "enigma-mapping"
                ],
                "extensions": [
                    ".mapping"
                ],
                "configuration": "./enigma-mapping-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tinyv2",
                "scopeName": "text.tinyv2",
                "path": "./syntaxes/tinyv2.tmLanguage.json"
            },
            {
                "language": "enigma-mapping",
                "scopeName": "text.enigma-mapping",
                "path": "./syntaxes/enigma-mapping.tmLanguage.json"
            }
        ]
    }
}
